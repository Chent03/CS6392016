<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffd"
    android:rowCount="4"
    android:columnCount="2"
    tools:context="chent03.imageviewer.MainActivityFragment"
    tools:showIn="@layout/activity_main">
    <ImageView
       android:id="@+id/Husky"
       android:layout_width="127dp"
       android:layout_height="127dp"
       android:src="@mipmap/ic_husky"
        android:layout_row="0"
        android:layout_column="0"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:scaleType="fitCenter" />
    <ImageView
        android:layout_height="127dp"
        android:layout_width="127dp"
        android:src="@mipmap/ic_retreiver"
        android:layout_row="0"
        android:layout_column="1"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:scaleType="fitCenter" />

    <ImageView
        android:layout_width="127dp"
        android:layout_height="127dp"
        android:id="@+id/imageView7"
        android:layout_row="1"
        android:layout_column="0"
        android:src="@mipmap/ic_pit"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:scaleType="fitCenter" />

    <ImageView
        android:layout_width="127dp"
        android:layout_height="127dp"
        android:id="@+id/imageView6"
        android:layout_row="1"
        android:layout_column="1"
        android:src="@mipmap/ic_corgi"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:scaleType="fitCenter" />

    <ImageView
        android:layout_height="127dp"
        android:layout_width="127dp"
        android:src="@mipmap/ic_french"
        android:layout_row="2"
        android:layout_column="0"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:scaleType="fitCenter" />


</GridLayout>
